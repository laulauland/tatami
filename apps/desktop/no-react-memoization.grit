`$fn($args)` where {
    or {
        $fn <: `useCallback`,
        $fn <: `useMemo`,
        $fn <: `memo`
    },
    register_diagnostic(
        span = $fn,
        message = "Manual memoization (useCallback/useMemo/memo) is unnecessary with React Compiler. Use regular functions/components instead.",
        severity = "warn"
    )
}
